<Application
    x:Class="Vantus.App.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Vantus.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
            </ResourceDictionary.MergedDictionaries>
            <local:Converters.IntToVisibilityConverter x:Key="CountToVisibilityConverter" xmlns:local="using:Vantus.App"/>
            <converters:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" xmlns:converters="using:Vantus.App.Converters"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
